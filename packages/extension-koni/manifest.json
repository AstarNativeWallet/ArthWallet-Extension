{
  "author": "ArthWallet Labo",
  "description": "Web3 Multiverse Gateway for Astar & Shiden Ecosystem",
  "homepage_url": "https://twitter.com/ArthWallet",
  "name": "ArthWallet - Astar Extension Wallet",
  "short_name": "ArthWallet",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "ArthWallet",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "128": "images/ArthWallet_logo.png"
  },
  "web_accessible_resources": [
    "page.js"
  ],
  "content_security_policy": "script-src 'self' blob: 'unsafe-eval' 'wasm-eval'; object-src 'self'",
  "version": "0.0.1"
}
